FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue Cd 0
    volScalarFieldValue LAD 0
    volScalarFieldValue Moisture 0
);

region
(

    boxToCell
    {
        box (1000 0 7) (1500 1 20);
        fieldValues
        (
            volScalarFieldValue LAD 0.14
            volScalarFieldValue Cd 0.2
        );
    }
);  




