FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType      RAS;

RAS
{
    RASModel        kEpsilon;

    turbulence      on;

    printCoeffs     on;

    kEpsilonCoeffs
    {
        Cmu             0.09;
        betaStar        0.09;
        sigmaEps        1.30; //Standard value - see Sogachev et al 2006
        sigmaK          1; //Standard value
        C1              1.44; //Standard value
        C2              1.92; //Standard value
    }
}


